<%= simple_form_for [:admin, @financial_entry] do |f| %>
  <div class="card mb-3">
    <div class="card-header">Importar arquivo</div>
    <div class="card-body">
      <div class="row">
        <div class="col-md-6">
          <%= f.association :user %>
        </div>

        <div class="col-md-6">
          <%= f.input :content_file, as: :file %>
        </div>
      </div>

    </div>
  </div>

  <%= render 'shared/form_actions', f: f, back_url: back_url %>
<% end %>
